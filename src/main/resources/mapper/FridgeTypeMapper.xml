<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.inthefridges.api.repository.FridgeTypeRepository">

    <insert id="create" parameterType="Fridge" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO fridge
        (
            fridge_id,
            type_id
        )
        VALUES
        (
            #{fridgeId},
            #{typeId}
        )
    </insert>

</mapper>
