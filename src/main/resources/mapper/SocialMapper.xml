<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.inthefridges.api.repository.SocialRepository">
    <select id="findBySocialName" resultType="Social">
        SELECT * FROM social
        WHERE name = #{socialName}
    </select>
</mapper>
